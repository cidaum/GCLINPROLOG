abrir(File) :-       
          open(File, read, Stream), 
		  see(Stream),
	get_char(Data),
	processar(Data),
          ler(Data).

ler(end_of_file).
 
ler(_) :-
  get_char(Data),
  processar(Data),
  ler(Data).

processar(end_of_file).

processar(' ').

processar(13).

processar(Data) :-  put_char(Data). 